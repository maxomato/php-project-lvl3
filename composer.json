        "doctrine/dbal": "^2.10",
        "imangazaliev/didom": "^1.14"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "squizlabs/php_codesniffer": "^3.5",
        "barryvdh/laravel-debugbar": "^3.2"
    }
